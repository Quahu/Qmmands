{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filter.yml",
      "properties": {
        "TargetFramework": "netcoreapp2.2"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site"
  }
}